html
  head
    link(rel="stylesheet" href="/css/styles.css")
  body 
    h1=showdata.title 
      h2 Developed By
      ul 
      for developed in showdata.developedBy  
        li= developed
      h2 Starring
      ul
      for stars in showdata.starring
        li= stars 
      h2 Seasons
      ul
      for season in showdata.seasons
        a(href=`/season/${season.number}`)
          b Season
          li= season.number
      